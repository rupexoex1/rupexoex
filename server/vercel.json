{
  "version": 2,
  "functions": {
    "src/index.js": {
      "runtime": "nodejs20.x",
      "regions": ["bom1"],
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "src/index.js" }
  ]
}
